{
  "cli": {
    "version": ">= 7.6.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "preview"
    },
    "staging": {
      "developmentClient": false,
      "distribution": "internal",
      "channel": "staging"
    },
    "production": {
      "developmentClient": false,
      "distribution": "store",
      "channel": "production",
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  },
  "hooks": {
    "eas-build-post-install": {
      "path": "./eas-build-post-install.sh"
    }
  }
}
