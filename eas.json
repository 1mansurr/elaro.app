{
  "cli": {
    "version": ">= 7.6.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "capabilities": {
          "pushNotifications": true
        }
      }
    },
    "preview": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "developmentClient": false,
      "distribution": "store",
      "channel": "production",
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
