{
  "name": "elaro",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "prebuild": "expo prebuild",
    "prebuild:ios": "expo prebuild --platform ios --clean",
    "prebuild:android": "expo prebuild --platform android --clean",
    "prebuild:clean": "expo prebuild --clean",
    "start": "expo start",
    "start:ios": "expo start --ios",
    "start:android": "expo start --android",
    "start:web": "expo start --web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:all": "eas build --platform all --profile production",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "submit:all": "eas submit --platform all",
    "clean": "rm -rf ios/build android/build android/app/build node_modules && npm install",
    "clean:ios": "rm -rf ios/build && npx expo run:ios --clear-cache",
    "clean:android": "rm -rf android/build android/app/build && npx expo run:android --clear-cache",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:typecheck": "eslint --config eslint.config.typecheck.js src/",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "audit-secrets": "node scripts/audit-secrets.js",
    "validate-env": "node -e \"require('./app.config.js')({config:{}})\"",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --config jest.config.integration.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --config jest.config.coverage.js",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:critical": "jest --testPathPattern='(auth|rls|security|permissions)' --bail --ci",
    "test:rls": "jest --runTestsByPath tests/rls/assignments.rls.test.ts",
    "test:rls:all": "jest --testPathPattern=tests/rls",
    "test:contracts": "jest --testPathPattern=tests/contracts",
    "test:edge-functions": "node scripts/test-edge-functions.js",
    "test:edge-functions:check-username": "node scripts/test-edge-functions.js check-username",
    "test:edge-functions:start-trial": "node scripts/test-edge-functions.js start-trial",
    "e2e:build:ios": "detox build --configuration ios.debug",
    "e2e:test:ios": "detox test --configuration ios.debug",
    "e2e:test:ci": "detox test --configuration ios.release --headless",
    "e2e:test:setup": "detox test --configuration ios.debug e2e/pass1-setup-verification.test.ts",
    "e2e:report:html": "node scripts/generate-html-report.js",
    "e2e:build:android": "detox build --configuration android.debug",
    "e2e:test:android": "detox test --configuration android.debug",
    "e2e:build:android:release": "detox build --configuration android.release",
    "e2e:test:android:release": "detox test --configuration android.release",
    "migrate-colors": "node scripts/migrate-colors.js",
    "migrate-colors:dry": "node scripts/migrate-colors.js --dry-run",
    "migrate-colors:fix": "node scripts/migrate-colors.js --fix",
    "migrate-colors:stats": "node scripts/migrate-colors.js --stats",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "audit:check": "npm audit --audit-level=moderate --production",
    "audit:imports": "node scripts/audit-imports.js",
    "verify:flatlist": "node scripts/verify-flatlist-optimizations.js",
    "validate:structure": "node scripts/validate-feature-structure.js",
    "analyze:bundle": "node scripts/analyze-bundle-size.js",
    "version:patch": "node scripts/version-manager.js patch",
    "version:minor": "node scripts/version-manager.js minor",
    "version:major": "node scripts/version-manager.js major",
    "version:build": "node scripts/version-manager.js build",
    "version:build:ios": "node scripts/version-manager.js build ios",
    "version:build:android": "node scripts/version-manager.js build android",
    "version:verify": "node scripts/verify-build-number.js",
    "version:current": "node -e \"console.log(require('./package.json').version)\"",
    "postinstall": "patch-package",
    "prepare": "husky",
    "eas-build-post-install": "./eas-build-post-install.sh"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@expo/metro-config": "~0.19.0",
    "@expo/metro-runtime": "~4.0.1",
