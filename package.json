{
  "name": "elaro",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "prebuild": "expo prebuild",
    "prebuild:ios": "expo prebuild --platform ios --clean",
    "prebuild:android": "expo prebuild --platform android --clean",
    "prebuild:clean": "expo prebuild --clean",
    "start": "expo start",
    "start:ios": "expo start --ios",
    "start:android": "expo start --android",
    "start:web": "expo start --web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:all": "eas build --platform all --profile production",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "submit:all": "eas submit --platform all",
    "clean": "rm -rf ios/build android/build android/app/build node_modules && npm install",
    "clean:ios": "rm -rf ios/build && npx expo run:ios --clear-cache",
    "clean:android": "rm -rf android/build android/app/build && npx expo run:android --clear-cache",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "e2e:build:ios": "detox build --configuration ios.debug",
    "e2e:test:ios": "detox test --configuration ios.debug",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@expo/metro-config": "~0.18.11",
    "@expo/metro-runtime": "~3.2.3",
    "@expo/vector-icons": "^14.1.0",
    "@invertase/react-native-apple-authentication": "^2.4.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/cli-platform-android": "^20.0.2",
    "@react-native-community/cli-platform-ios": "^20.0.2",
    "@react-native-community/cli-server-api": "^20.0.2",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.3.29",
    "@sentry/react-native": "~5.24.3",
    "@supabase/supabase-js": "^2.50.3",
    "@tanstack/react-query": "^5.90.2",
    "@types/react-native-vector-icons": "^6.4.18",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
